	<table class="table table-striped">
		<thead>
			<tr>
				<th class = "test">Date</th>
				<th>Sleep time</th>
				<th>Fell Asleep</th>
				<th>Woke Up</th>
			</tr>
		</thread>
		<tbody>
			<% for (var i=0; i<items.length; i++) { %>
				<tr>
					<td><%= items[i].date %></td>
					<td><%= items[i].title %></td>
					<td><%= items[i].time_created %></td>
					<td><%= items[i].time_completed %></td>
				</tr>
			<% } %> 
		</tbody>
	</table>
</body>
